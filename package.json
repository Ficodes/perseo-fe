  },
  "bugs": {
    "url": "https://github.com/telefonicaid/perseo-fe/issues"
  },
  "main": "bin/perseo",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "clean": "rm -rf package-lock.json && rm -rf node_modules && rm -rf coverage",
    "test": "mocha --recursive 'test/**/*.js' --reporter spec --timeout 3000 --ui bdd --exit",
    "test:watch": "npm run test -- -w ./lib",
    "lint": "jshint lib/ --config .jshintrc && jshint test/ --config test/.jshintrc",
    "test:coverage": "istanbul cover _mocha -- --recursive 'test/**/*.js' --reporter spec --exit",
    "watch": "watch 'npm test && npm run lint' ./lib ./test"
  },
  "devDependencies": {
    "chai": "~4.1.2",
    "istanbul": "~0.4.5",
    "jshint": "~2.9.6",
    "mocha": "5.2.0",
    "proxyquire": "2.0.1",
    "rewire": "~4.0.1",
    "should": "8.2.2",
    "watch": "~1.0.2",
    "sinon": "~6.1.0",
    "sinon-chai": "3.2.0"
  },
  "keywords": [],
  "dependencies": {
    "async": "~0.9.2",
    "body-parser": "~1.18.2",
    "closure-linter-wrapper": "0.2.11",
    "express": "~4.16.1",
    "logops": "2.1.0",
    "mongodb": "~2.2.31",
    "ngsijs": "^1.2.0",
    "nodemailer": "~4.6.7",
    "nodemailer-smtp-transport": "~2.7.4",
    "request": "~2.87.0",
    "twitter": "~1.7.1",
    "utm-converter": "~0.1.1",
    "uuid": "~1.4.1",
    "smpp": "0.3.1"
  }
}
